{
  "Amends": {
    "prefix": "amends",
    "body": "amends \"${1:moduleUri}\""
  },
  "Class": {
    "prefix": "class",
    "body": "class ${1:className} {\n}"
  },
  "Extends": {
    "prefix": "extends",
    "body": "extends \"${1:moduleUri}\""
  },
  "Function": {
    "prefix": "function",
    "body": "function ${1:functionName}(${2:parameterName}: ${3:parameterType}): ${4:returnType} = $0"
  },
  "If-Else": {
    "prefix": "if-else",
    "body": "if (${1:condition}) $2 else $0"
  },
  "Import": {
    "prefix": "import",
    "body": "import \"${1:moduleUri}\""
  },
  "Glob import": {
    "prefix": "import*",
    "body": "import* \"${1:globPattern}\""
  },
  "Read": {
    "prefix": "read",
    "body": "read(\"${1:resourceUri}\")"
  },
  "Nullable read": {
    "prefix": "read?",
    "body": "read?(\"${1:resourceUri}\")"
  },
  "Glob read": {
    "prefix": "read*",
    "body": "read*(\"${1:globPattern}\")"
  },
  "Lambda": {
    "prefix": "->",
    "body": "(${1:parameters}) -> $0"
  },
  "Throw": {
    "prefix": "throw",
    "body": "throw(\"${1:message}\")"
  }
}
